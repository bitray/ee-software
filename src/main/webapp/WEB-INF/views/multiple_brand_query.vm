<script type="text/javascript">
	$(document).ready(function() {
		var malltableButton = function() {
			if ($("#malltable").length) {
				$("#malltable").DataTable({
					dom : "Bfrtip",
					buttons : [ {
						extend : "excel",
						className : "btn-sm"
					} ],
					responsive : true
				});
			}
		};

		var floortableButton = function() {
			if ($("#floortable").length) {
				$("#floortable").DataTable({
					dom : "Bfrtip",
					buttons : [ {
						extend : "excel",
						className : "btn-sm"
					} ],
					responsive : true
				});
			}
		};

		TableManageButtons = function() {
			"use strict";
			return {
				init : function() {
					malltableButton();
					floortableButton();
				}
			};
		}();

		TableManageButtons.init();
	});
</script>
