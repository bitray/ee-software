<script>
	$(document).ready(
			function() {
				//random data
				var d1 = [ [ 0, 4 ], [ 1, 9 ], [ 2, 6 ], [ 3, 10 ], [ 4, 5 ],
						[ 5, 17 ], [ 6, 6 ], [ 7, 10 ], [ 8, 7 ], [ 9, 11 ],
