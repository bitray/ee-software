				var plot = $.plot($("#placeholder3xx3"), [ {
					label : "Registrations",
					data : d1,
					lines : {
						fillColor : "rgba(150, 202, 89, 0.12)"
					}, //#96CA59 rgba(150, 202, 89, 0.42)
					points : {
						fillColor : "#fff"
					}
				} ], options);

				var plot2 = $.plot($("#graph_bar"), [ {
					label : "Registrations1",
					data : d1,
					lines : {
						fillColor : "rgba(150, 202, 89, 0.12)"
